    "keywords": ["silex", "validator", "renegare", "skip"],
    "license": "BWL",
    "authors": [
        {
            "name": "Mudi Ugbowanko",
            "email": "mudi@renegare.com"
        }
    ],

    "minimum-stability": "stable",

    "autoload": {
